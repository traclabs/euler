<?xml version="1.0" ?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:macro name="vetex" params="prefix">

  <property name="base_x_disp" value="1.0438"/>
  <property name="base_z_disp" value="0.3208"/>

    <!-- link list -->
    <link name="${prefix}base_link">
      <visual>
        <geometry>
          <mesh filename="package://vetex_support/meshes/collision/base_link_centered.stl" />
        </geometry>
        <material name="euler_gray">
          <color rgba="0.75294 0.75294 0.75294 1" />
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://vetex_support/meshes/collision/base_link_centered.stl" />
        </geometry>
      </collision>
      <inertial>
        <mass value="52.0"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="0.1347" ixy="0.0" ixz="0.0" iyy="0.5214" iyz="0.0" izz="0.6144"/>
      </inertial>
    </link>
    <link name="${prefix}base_footprint"/>
    <link name="${prefix}left_front_wheel">
      <visual>
        <geometry>
          <!--<cylinder length="0.2" radius="0.2"/>-->
          <mesh filename="package://vetex_support/meshes/visual/left_front_wheel.stl" />
        </geometry>
        <material name="euler_black">
          <color rgba="0.01 0.01 0.01 1" />
        </material>
      </visual>
      <collision>
        <origin xyz="0 -0.10 0" rpy="1.5708 0 0" />
        <geometry>
          <cylinder length="0.2" radius="0.2"/>
          <!--<mesh filename="package://vetex_support/meshes/collision/left_front_wheel.stl" /> -->
        </geometry>
      </collision>
      <inertial>
        <mass value="5.8517"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="0.02467" ixy="0.0" ixz="0.0" iyy="0.04411" iyz="0.0" izz="0.02467"/>
      </inertial>  
    </link>                
    <link name="${prefix}left_rear_wheel">
      <visual>
        <geometry>
          <!--<cylinder length="0.2" radius="0.2"/>-->
          <mesh filename="package://vetex_support/meshes/visual/left_rear_wheel.stl" />
        </geometry>
        <material name="euler_black"/>
      </visual>
      <collision>
        <origin xyz="0 -0.10 0" rpy="1.5708 0 0" />
        <geometry>
          <cylinder length="0.2" radius="0.2"/>
          <!-- <mesh filename="package://vetex_support/meshes/collision/left_rear_wheel.stl" /> -->
        </geometry>
      </collision>
      <inertial>
        <mass value="5.8517"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="0.02467" ixy="0.0" ixz="0.0" iyy="0.04411" iyz="0.0" izz="0.02467"/>
      </inertial>
    </link>
    <link name="${prefix}right_rear_wheel">
      <visual>
        <origin rpy="3.1415 0 0" />
        <geometry>
          <!--<cylinder length="0.2" radius="0.2"/>-->
          <mesh filename="package://vetex_support/meshes/visual/right_rear_wheel.stl" />
        </geometry>
        <material name="euler_black"/>
        </visual>
      <collision>
        <origin xyz="0 0. 0" rpy="1.5708 0 0" />
        <!--<origin rpy="3.1415 0 0" />-->
        <geometry>
          <cylinder length="0.2" radius="0.2"/>
          <!-- <mesh filename="package://vetex_support/meshes/collision/right_rear_wheel.stl" /> -->
        </geometry>
      </collision>
      <inertial>
        <mass value="5.8517"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="0.02467" ixy="0.0" ixz="0.0" iyy="0.04411" iyz="0.0" izz="0.02467"/>
      </inertial>  
    </link>
    <link name="${prefix}right_front_wheel">
      <visual>
        <origin rpy="3.1415 0 0" />
        <geometry>
          <!--<cylinder length="0.2" radius="0.2"/>-->
          <mesh filename="package://vetex_support/meshes/visual/right_front_wheel.stl" />
        </geometry>
        <material name="euler_black"/>
      </visual>
      <collision>
        <origin xyz="0 0.10 0" rpy="1.5708 0 0" />
        <!--<origin rpy="3.1415 0 0" />-->    
        <geometry> 
          <cylinder length="0.2" radius="0.2"/>
          <!-- <mesh filename="package://vetex_support/meshes/collision/right_front_wheel.stl" /> -->
        </geometry>
      </collision>
      <inertial>
        <mass value="5.8517"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="0.02467" ixy="0.0" ixz="0.0" iyy="0.04411" iyz="0.0" izz="0.02467"/>
      </inertial>
    </link>
    <!-- end of link list -->
    <!-- joint list -->
    <joint name="${prefix}to_footprint" type="fixed">
      <origin xyz="0 0 ${base_z_disp}"/>
      <parent link="${prefix}base_footprint" />
      <child link="${prefix}base_link" />
    </joint>
    <joint name="${prefix}left_front_wheel_joint" type="continuous">
      <!--<origin xyz="0 0.58668 0.21577" rpy="1.57 1 0" />-->
      <origin xyz="${base_x_disp + 0} 0.58668 ${0.21577-base_z_disp}" rpy="0 0 0" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}left_front_wheel" />
      <axis xyz="0 1 0" />
      <limit effort="20" velocity="3" />
    </joint>
    <joint name="${prefix}left_rear_wheel_joint" type="continuous">
      <!--<origin xyz="-2.0878 0.58642 0.21577" rpy="1.57 1 0" />-->
      <origin xyz="${base_x_disp -2.0878} 0.58642 ${0.21577-base_z_disp}" rpy="0 0 0" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}left_rear_wheel" />
      <axis xyz="0 1 0" />
      <limit effort="20" velocity="3" />
    </joint>
    <joint name="${prefix}right_rear_wheel_joint" type="continuous">
      <!--<origin xyz="-2.0878 -0.58668 0.21577" rpy="1.57 1 0" />-->
      <origin xyz="${base_x_disp -2.0878} -0.58668 ${0.21577-base_z_disp}" rpy="0 0 0" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}right_rear_wheel" />
      <axis xyz="0 1 0" />
      <limit effort="20" velocity="3" />
    </joint>                
    <joint name="${prefix}right_front_wheel_joint" type="continuous">
      <!--<origin xyz="0 -0.58642 0.21577" rpy="1.57 1 0" />-->
      <origin xyz="${base_x_disp + 0} -0.58642 ${0.21577 - base_z_disp}" rpy="0 0 0" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}right_front_wheel" />
      <axis xyz="0 1 0" />
      <limit effort="20" velocity="3" />
    </joint>
    <!-- end of joint list -->
  </xacro:macro>
</robot>
